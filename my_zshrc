#POWERLEVEL9K_LEFT_SUBSEGMENT_SEPARATOR='%F{008}\u26A1%F{008}'

# command line rhs: # <= right prompt "time"
POWERLEVEL9K_RIGHT_PROMPT_ELEMENTS=(status virtualenv time) 
