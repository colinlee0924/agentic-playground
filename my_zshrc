# Path to your oh-my-zsh installation.
export ZSH="/Users/lizonglin/.oh-my-zsh"

# Set name of the theme to load --- if set to "random", it will
# load a random theme each time oh-my-zsh is loaded, in which case,
# to know which specific one was loaded, run: echo $RANDOM_THEME
# See https://github.com/ohmyzsh/ohmyzsh/wiki/Themes
ZSH_THEME="powerlevel9k/powerlevel9k"

POWERLEVEL9K_MODE='nerdfont-complete'
POWERLEVEL9K_PROMPT_ON_NEWLINE=true
POWERLEVEL9K_RPROMPT_ON_NEWLINE=true
POWERLEVEL9K_PROMPT_ADD_NEWLINE=true
POWERLEVEL9K_RPROMPT_ADD_NEWLINE=true

POWERLEVEL9K_MULTILINE_FIRST_PROMPT_PREFIX="↱"
#POWERLEVEL9K_MULTILINE_FIRST_PROMPT_PREFIX=""
POWERLEVEL9K_MULTILINE_LAST_PROMPT_PREFIX="↳ "
#POWERLEVEL9K_MULTILINE_LAST_PROMPT_PREFIX="$ "

DEFAULT_USER="lizonglin"

POWERLEVEL9K_DIR_HOME_SUBFOLDER_BACKGROUND='221'
POWERLEVEL9K_DIR_HOME_BACKGROUND='221'

POWERLEVEL9K_VCS_CLEAN_BACKGROUND='067'
POWERLEVEL9K_VCS_UNTRACKED_BACKGROUND='067'
POWERLEVEL9K_VCS_MODIFIED_BACKGROUND='067'

POWERLEVEL9K_TIME_BACKGROUND='214'

POWERLEVEL9K_CUSTOM_DOLLAR="printf $"
POWERLEVEL9K_CUSTOM_DOLLAR_BACKGROUND='237'
POWERLEVEL9K_CUSTOM_DOLLAR_FOREGROUND='195'

# show on the left side of command line "dir + vcs"
